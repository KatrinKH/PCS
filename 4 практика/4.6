class T6
{
    static void Main(string[] args)
    {
        int D = 100;
        int K = 7;
        int[] A = {1,3,15,65,412,909,1234,9999,10024};

        int indexD = Array.FindIndex(A, x => x > D);

        var firstFragment = A.TakeWhile((x, index) => index < indexD);
        var secondFragment = A.Skip(K - 1);

        var mergedList = firstFragment.Concat(secondFragment);
        var uniqueList = mergedList.Distinct();
        var sortedList = uniqueList.OrderByDescending(x => x);

        foreach (var str in sortedList)
        {
            Console.WriteLine(str);
        }
    }
}
